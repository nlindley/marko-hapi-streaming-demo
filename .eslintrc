{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-inner-declarations": [
            2,
            "both"
        ],
        "curly": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "dot-notation": 2,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-labels": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-proto": 2,
        "no-return-assign": [2, "always"],
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-with": 2,
        "radix": 2,
        "wrap-iife": [2, "inside"],
        "yoda": 2,
        "strict": [2, "global"],
        "no-shadow-restricted-names": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "array-bracket-spacing": [2, "never"],
        "eol-last": 2,
        "no-trailing-spaces": 2,
        "no-unneeded-ternary": 2,
        "keyword-spacing": ["error"],
        "no-multiple-empty-lines": [2, {max: 1}],
        "block-spacing": 2,
        "brace-style": 2,
        "computed-property-spacing": [2, "never"],
        "func-style": [2, "expression", { "allowArrowFunctions": true }],
        "key-spacing": 2,
        //"func-names": 2,
        //"max-nested-callbacks": [2, 2],
        "new-parens": 2,
        "newline-after-var": 2,
        "no-array-constructor": 2,
        "no-continue": 2,
        "no-lonely-if": 2,
        "no-negated-condition": 2,
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "comma-spacing": 2,
        "one-var": [2, "never"],
        "quote-props": [2, "as-needed"],
        "prefer-const": 2,
        "prefer-template": 2,
        "no-var": 2,
        "no-const-assign": 2,
        "no-use-before-define": 2
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
